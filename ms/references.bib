
@article{renard_understanding_2024,
  title    = {Understanding prediction discrepancies in classification},
  volume   = {113},
  issn     = {1573-0565},
  url      = {https://doi.org/10.1007/s10994-024-06557-4},
  doi      = {10.1007/s10994-024-06557-4},
  abstract = {A multitude of classifiers can be trained on the same data to achieve similar performances during test time while having learned significantly different classification patterns. When selecting a classifier, the machine learning practitioner has no understanding on the differences between models, their limits, where they agree and where they don’t. But this choice will result in concrete consequences for instances to be classified in the discrepancy zone, since the final decision will be based on the selected classification pattern. Besides the arbitrary nature of the result, a bad choice could have further negative consequences such as loss of opportunity or lack of fairness. This paper proposes to address this question by analyzing the prediction discrepancies in a pool of best-performing models trained on the same data. A model-agnostic algorithm, DIG, is proposed to capture and explain discrepancies locally in tabular datasets, to enable the practitioner to make the best educated decision when selecting a model by anticipating its potential undesired consequences.},
  language = {en},
  number   = {10},
  urldate  = {2025-02-08},
  journal  = {Machine Learning},
  author   = {Renard, Xavier and Laugel, Thibault and Detyniecki, Marcin},
  month    = oct,
  year     = {2024},
  keywords = {Artificial Intelligence, Discrepancy, Explainability, Predictive multiplicity},
  pages    = {7997--8026},
  file     = {PDF:/Users/niche/Zotero/storage/G6QCPSMB/Renard et al. - 2024 - Understanding prediction discrepancies in classification.pdf:application/pdf}
}

@article{wang_investigating_2024,
  title    = {Investigating the deviation between prediction accuracy metrics and control performance metrics in the context of an ice-based thermal energy storage system},
  volume   = {91},
  issn     = {2352-152X},
  url      = {https://www.sciencedirect.com/science/article/pii/S2352152X24017122},
  doi      = {10.1016/j.est.2024.112126},
  abstract = {Extensive research on cooling load prediction has been conducted to improve the prediction accuracy. The existing studies mainly use statistical metrics to evaluate prediction accuracy. However, under these metrics, accurate cooling load prediction models do not always lead to better application performance of downstream control optimization. This study demonstrates the phenomenon in a real case of an ice-based thermal energy storage system. There are 180 cooling load prediction models from six different data-driven algorithms developed to test the prediction accuracy and control performance. Four prediction accuracy metrics are investigated, including mean absolute percentage error (MAPE), coefficient of variation of mean absolute error (CV-MAE), coefficient of variation of root mean squared error (CV-RMSE), and coefficient of variation of root weighted squared error (CV-RWSE). To quantitatively evaluate the deviation between prediction accuracy metrics and control performance metrics, the Spearman correlation coefficient is applied. The Spearman correlation coefficients of MAPE, CV-MAE, and CV-RMSE are +0.467, −0.490, and −0.743, respectively, which demonstrates that the prediction model with the highest accuracy does not always lead to the best control performance. It is crucial to customize prediction accuracy metrics for specific control problems. Spearman correlation coefficient can effectively evaluate the monotonicity and concentration between prediction accuracy and control performance, which can support better design of application-oriented prediction accuracy metrics and facilitate the interaction between cooling load prediction and control tasks.},
  urldate  = {2025-02-08},
  journal  = {Journal of Energy Storage},
  author   = {Wang, Xiao and Liu, Xue and Wang, Yanfang and Kang, Xuyuan and Geng, Ruoxi and Li, Ao and Xiao, Fu and Zhang, Changhao and Yan, Da},
  month    = jun,
  year     = {2024},
  keywords = {Control performance, Cooling load prediction, Data-driven model, Ice-based thermal energy storage, Prediction accuracy metric},
  pages    = {112126},
  file     = {PDF:/Users/niche/Zotero/storage/WWYT38QM/Wang et al. - 2024 - Investigating the deviation between prediction accuracy metrics and control performance metrics in t.pdf:application/pdf;ScienceDirect Snapshot:/Users/niche/Zotero/storage/N7N6L5HE/S2352152X24017122.html:text/html}
}

@article{ma_multisource_2023,
  title    = {Multisource {Maximum} {Predictor} {Discrepancy} for {Unsupervised} {Domain} {Adaptation} on {Corn} {Yield} {Prediction}},
  volume   = {61},
  issn     = {1558-0644},
  url      = {https://ieeexplore.ieee.org/document/10049448?denied=},
  doi      = {10.1109/TGRS.2023.3247343},
  abstract = {Recently, with the advent of satellite missions and artificial intelligence techniques, supervised machine learning (ML) methods have been more and more used for analyzing remote sensing (RS) observation data for crop yield prediction. However, due to the domain shift between heterogeneous regions, supervised ML models tend to have poor spatial transferability. As a result, models trained with labeled data from one spatial region (i.e., source domain) often lose their validity when directly applied to another region (i.e., target domain). To address this issue, we proposed a multisource maximum predictor discrepancy (MMPD) neural network that is an unsupervised domain adaptation (UDA) approach for corn yield prediction at the county level. The novelties of this study include that: 1) we proposed to maximize the discrepancy between two source-specific yield predictors and align source and target domains by considering crop yield response in the target domain and 2) we adopted the strategy of multisource UDA to avoid negative interference between labeled samples from different sources. Case studies in the U.S. corn belt and Argentina demonstrated that the proposed MMPD model had effectively reduced domain shifts and outperformed several other state-of-the-art deep learning (DL) and UDA methods.},
  urldate  = {2025-02-08},
  journal  = {IEEE Transactions on Geoscience and Remote Sensing},
  author   = {Ma, Yuchi and Yang, Zhengwei and Zhang, Zhou},
  year     = {2023},
  note     = {Conference Name: IEEE Transactions on Geoscience and Remote Sensing},
  keywords = {Adaptation models, Biological system modeling, Crop yield prediction, Crops, Data models, deep learning (DL), Feature extraction, multisource domain, Predictive models, satellite remote sensing (RS), Task analysis, unsupervised domain adaptation (UDA)},
  pages    = {1--15},
  file     = {IEEE Xplore Abstract Record:/Users/niche/Zotero/storage/85QNPQCS/10049448.html:text/html;PDF:/Users/niche/Zotero/storage/V6YP9NDW/Ma et al. - 2023 - Multisource Maximum Predictor Discrepancy for Unsupervised Domain Adaptation on Corn Yield Predictio.pdf:application/pdf}
}



